<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.citoneitor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Principal" />
        <activity android:name=".Activities.Clientes" />
        <activity android:name=".Activities.NuevoCliente" />
        <activity android:name=".Activities.NuevaCita" />
        <activity android:name=".Activities.Servicios" />
        <activity android:name=".Activities.NuevoServicio"></activity>
    </application>

</manifest>